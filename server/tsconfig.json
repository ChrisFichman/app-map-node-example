{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "lib": [
      "ES2021"
    ],
    "module": "ES2022",
    "moduleResolution": "node",
    "target": "ES2021",
    "allowJs": true,
    "checkJs": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "alwaysStrict": true,
    "noImplicitAny": true,
    "outDir": "dist"
  }
}